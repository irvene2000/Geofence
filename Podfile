# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

inhibit_all_warnings!

workspace 'Geofence.xcworkspace'
project 'Geofence.xcodeproj'

target 'Geofence' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Geofence
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
end

target 'GeofenceTests' do
  use_frameworks!
  
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
end
